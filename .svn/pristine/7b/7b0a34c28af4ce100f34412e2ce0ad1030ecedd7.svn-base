package com.sim.server.game.message.response.user;
/**
 * @author liwen
 * 2017年11月2日 下午4:19:01
 * 此类的功能:登录后获取房间信息的响应
 * @version 1.0
 */

import java.util.List;

import com.sim.server.game.common.ProtocolConstant;
/**
 * @author liwen
 * 2017年11月9日 下午3:28:46
 * Function: 登录成功后返回房间列表响应
 * @version 1.0
 */
public class LoginRoomResponse {

	private Integer len ; 
	
	private Short responseId = ProtocolConstant.LOGIN_ROOMLIST_RESPONSE_PROTOCOL; 
	
	private Integer roomCount; 
	
	private List<String> rooms ;

	public void setLen(Integer len) {
		this.len = len;
	}

	public Short getResponseId() {
		return responseId;
	}

	public void setResponseId(Short responseId) {
		this.responseId = responseId;
	}
	
	public Integer getRoomCount() {
		return roomCount;
	}

	public void setRoomCount(Integer roomCount) {
		this.roomCount = roomCount;
	}

	public List<String> getRooms() {
		return rooms;
	}

	public void setRooms(List<String> rooms) {
		this.rooms = rooms;
	}
	public Integer getLen() {
		return len;
	}
	@Override
	public String toString() {
		return "LoginRoomResponse [len=" + len + ", responseId=" + responseId + ", roomCount=" + roomCount + ", rooms="
				+ rooms + "]";
	}
	
	
}
