package com.lw.test;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public class TestB {
	
	private static byte[] cards = new byte[] {
			0x01,0x02,0x03,0x04,0x05,0x06,0x07,0x08,0x09,0x0A,0x0B,0x0C,0x0D,0x0E,// 黑桃A~黑桃K,大王
			0x11,0x12,0x13,0x14,0x15,0x16,0x17,0x18,0x19,0x1A,0x1B,0x1C,0x1D,0x1E,// t桃A~t桃K,小王
			0x21,0x22,0x23,0x24,0x25,0x26,0x27,0x28,0x29,0x2A,0x2B,0x2C,0x2D,// 梅花A~梅花K
			0x31,0x32,0x33,0x34,0x35,0x36,0x37,0x38,0x39,0x3A,0x3B,0x3C,0x3D,// 方片A~方片K
		} ;
	
	static List<Integer> list = new ArrayList<>() ;
	private static List<Byte> cardList = new ArrayList<>() ;
	
	public static void main(String[] args) {
		for (byte string : cards) {
			cardList.add(string);
		}
		Collections.shuffle(cardList);
		System.out.println(cardList);
		
		byte b = (byte) 11101;
		byte a = (byte) (b>>3);
		System.out.println(a-b);
		System.out.println("-----------------");
	}
}
